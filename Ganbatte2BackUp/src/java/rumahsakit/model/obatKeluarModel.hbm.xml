<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="rumahsakit.model.ObatKeluarModel" table="obat_keluar">
        <id name="id_obat_keluar" column="id_obat_keluar">
            <generator class="native"/>
        </id>
        <property name="id_obat" column="id_obat"/>
        <property name="id_pasien" column="id_pasien"/>
        <property name="tgl_obat_keluar" column="tgl_obat_keluar"/>
        <property name="harga_obat_keluar" column="harga_obat_keluar"/>
        <property name="tgl_pembayaran" column="tgl_pembayaran"/>
        <property name="jumlah_obat_keluar" column="jumlah_obat_keluar"/>
        <property name="status_obat" column="status_obat"/>
        
    </class>
  

</hibernate-mapping>
