<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="rumahsakit.model.PasienModel" table="pasien">
        <id name="id_pasien" column="Id_pasien">
            <generator class="native"/>
        </id>
        <property name="nama_pasien" column="nama_pasien"/>
        <property name="tgl_lahir_pasien" column="tgl_lahir_pasien"/>
        <property name="jenis_kelamin" column="jenis_kelamin"/>
        <property name="alamat" column="alamat"/>
        <property name="agama" column="agama"/>
        <property name="keterangan" type="text" column="keterangan"/>
    </class>
  

</hibernate-mapping>
