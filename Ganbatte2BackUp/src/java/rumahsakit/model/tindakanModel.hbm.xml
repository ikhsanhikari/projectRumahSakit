<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="rumahsakit.model.TindakanModel" table="tindakan">
        <id name="id_tindakan" column="id_tindakan">
            <generator class="native"/>
        </id>
        <property name="id_klinik" column="id_klinik"/>
        <property name="id_pasien" column="id_pasien"/>
        <property name="nama_tindakan" column="nama_tindakan"/>
    </class>
</hibernate-mapping>
