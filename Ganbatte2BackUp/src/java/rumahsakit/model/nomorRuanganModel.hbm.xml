<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="rumahsakit.model.NomorRuanganModel" table="nomor_ruangan">
        <id name="id_nomor_ruangan" column="id_nomor_ruangan">
            <generator class="native"/>
        </id>
        <property name="id_ruangan" column="id_ruangan"/>
        <property name="nomor_ruangan" column="nomor_ruangan"/>
        <property name="status_ruangan" column="status_ruangan"/>
    </class>    
  

</hibernate-mapping>
