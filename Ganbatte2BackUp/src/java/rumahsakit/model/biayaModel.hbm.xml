<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="rumahsakit.model.BiayaModel" table="biaya">
        <id name="id_biaya" column="id_biaya">
            <generator class="native"/>
        </id>
        <property  name="id_pasien" column="id_pasien"/>
        <property name="biaya_ruangan" column="biaya_ruangan"/>
        <property name="biaya_ambulance" column="biaya_ambulance"/>
        <property name="biaya_administrasi" column="biaya_administrasi"/>
        <property name="biaya_dokter" column="biaya_dokter"/>
        <property name="biaya_observasi" column="biaya_observasi"/>
        <property name="biaya_tindakan" column="biaya_tindakan"/>
        <property name="biaya_visit_dokter" column="biaya_visit_dokter"/>
        <property name="biaya_visit_spesialis" column="biaya_visit_spesialis"/>
        <property name="biaya_perawatan" column="biaya_perawatan"/>
        <property name="biaya_lab" column="biaya_lab"/>
        <property name="biaya_lain_lain" column="biaya_lain_lain"/>
        <property name="total_biaya" column="total_biaya"/>
    </class>
  

</hibernate-mapping>
