<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="rumahsakit.model.RuanganModel" table="ruangan">
        <id name="id_ruangan" column="id_ruangan">
            <generator class="native"/>
        </id>
        <property name="jenis_ruangan" column="jenis_ruangan"/>
        <property name="harga_ruangan" column="harga_ruangan"/>
    </class>
</hibernate-mapping>
